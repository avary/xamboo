{
  "listeners":
  [
    {
      "name": "admin",
      "ip": "",
      "port": "81",
      "protocol": "https",
      "readtimeout": 120,
      "writetimeout": 120,
      "headersize": 65536
    },
    {
      "name": "https",
      "ip": "",
      "port": "82",
      "protocol": "https",
      "readtimeout": 120,
      "writetimeout": 120,
      "headersize": 65536
    }
  ],

  "hosts":
  [
    {
      "name": "developers",
      "listeners": ["https"],
      "hostnames": ["developers.webability.info"],
      "config": ["./example/application/config/example.conf"],
      "cert": "/etc/letsencrypt/live/developers.webability.info/cert.pem",
      "key": "/etc/letsencrypt/live/developers.webability.info/privkey.pem",
      "static": "./example/repository/public/static"
    },
    {
      "name": "devapi-for-cors-api",
      "listeners": ["https"],
      "hostnames": ["devapi.webability.info"],
      "config": ["./example/application/config/example.conf"],
      "cert": "/etc/letsencrypt/live/developers.webability.info/cert.pem",
      "key": "/etc/letsencrypt/live/developers.webability.info/privkey.pem",
      "static": "./example/repository/public/static",

      "origin": {
        "maindomain": "webability.info",
        "defaultdomain": "https://developers.webability.info",
        "methods": ["GET", "POST", "OPTIONS"],
        "headers": ["Accept", "Content-Type", "Content-Length", "Accept-Encoding", "X-CSRF-Token", "Authorization", "Origin", "X-Requested-With", "Method"],
        "credentials": true
      }
      
    },
    {
      "name": "admin",
      "listeners": ["admin"],
      "hostnames": ["developers.webability.info"],
      "config": ["./admin/admin.conf"],
      "cert": "/etc/letsencrypt/live/developers.webability.info/cert.pem",
      "key": "/etc/letsencrypt/live/developers.webability.info/privkey.pem",
      "static": "./admin/public"
    }
  ],
  
  "engines":
  [
    { "name": "simple" },
    { "name": "library" },
    { "name": "template" },
    { "name": "language" }
  ]
}
